{
  "name": "root",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "lerna run build",
    "postbuild": "run-s docs",
    "build:annotations": "lerna run build --scope='*/annotations'",
    "build:axes": "lerna run build --scope='*/axes'",
    "build:charts": "lerna run build --scope='reincharts'",
    "build:coordinates": "lerna run build --scope='*/coordinates'",
    "build:core": "lerna run build --scope='*/core'",
    "build:indicators": "lerna run build --scope='*/indicators'",
    "build:interactive": "lerna run build --scope='*/interactive'",
    "build:scales": "lerna run build --scope='*/scales'",
    "build:series": "lerna run build --scope='*/series'",
    "build:tooltip": "lerna run build --scope='*/tooltip'",
    "build:utils": "lerna run build --scope='*/utils'",
    "clean": "run-s clean:packages clean:modules",
    "clean:packages": "lerna run clean",
    "clean:modules": "lerna clean --yes",
    "docs": "run-s docs:stories docs:typedoc",
    "docs:stories": "lerna run docs --stream",
    "docs:typedoc": "typedoc && run-s docs:typedoc:set-base",
    "docs:typedoc:set-base": "perl -pi -e 's|<head><meta charset=\"utf-8\"/>|<head><meta charset=\"utf-8\"/><base href=\"/typedocs/\"/>|' typedocs/index.html",
    "prebuild:web": "run-s build",
    "build:web": "lerna run build:web --stream",
    "start:web": "lerna run start:web --stream",
    "lint": "eslint . --ext ts --ext tsx --quiet",
    "lint:fix": "eslint . --ext ts --ext tsx --quiet --fix",
    "prestart": "npm run build",
    "start": "lerna run start --stream",
    "prepublishOnly": "npm run build",
    "publish": "lerna publish from-package --yes",
    "watch": "lerna run watch --parallel"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/d3-array": "^3.2.1",
    "@types/d3-dsv": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-format": "^3.0.4",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-path": "^3.1.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time-format": "^4.0.3",
    "@types/lodash.flattendeep": "^4.4.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "lerna": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.9",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@reincharts/annotations": "file:packages/annotations",
    "@reincharts/axes": "file:packages/axes",
    "@reincharts/coordinates": "file:packages/coordinates",
    "@reincharts/core": "file:packages/core",
    "@reincharts/indicators": "file:packages/indicators",
    "@reincharts/interactive": "file:packages/interactive",
    "@reincharts/scales": "file:packages/scales",
    "@reincharts/series": "file:packages/series",
    "@reincharts/stories": "file:packages/stories",
    "@reincharts/tooltip": "file:packages/tooltip",
    "@reincharts/utils": "file:packages/utils",
    "reincharts": "file:packages/charts"
  }
}
